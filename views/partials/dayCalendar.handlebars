<div id="week-calendar">
	<table>
        <thead>
            <tr>
                <th class="table-name">Name</th>
                {{#each data.dayHours}}
				<th class = "table-day">
					{{this}}
				</th>
				{{/each}}
            </tr>
        </thead>
		<tbody>
			{{#each data.rows}}
            <tr>
                <td>{{name}}</td>
                {{#hours}}
                    {{#if this}}
                    	{{#each this}}
						<td class = "days-with-schedule">
							{{this}}
						</td>
						{{/each}}
					{{else}}
					<td></td>
					{{/if}}
                {{/hours}}

            </tr>
            {{/each}}
        </tbody>
    </table>
</div>